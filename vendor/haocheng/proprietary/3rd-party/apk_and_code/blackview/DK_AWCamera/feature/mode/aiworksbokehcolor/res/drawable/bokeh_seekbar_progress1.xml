<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <color android:color="@color/bokeh_seekbar_progress_color2" />
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dip" />
                <gradient
                        android:startColor="@color/bokeh_seekbar_progress_color1"
                        android:centerColor="@color/bokeh_seekbar_progress_color1"
                        android:endColor="@color/bokeh_seekbar_progress_color1"
                />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <color android:color="@color/bokeh_seekbar_progress_color2" />
    </item>
</layer-list>
